    "debug": "^4.3.1",
    "docker-compose": "^0.23.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "iocane": "^4.2.0",
    "lndconnect": "^0.2.10",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "node-rsa": "^1.1.1",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
